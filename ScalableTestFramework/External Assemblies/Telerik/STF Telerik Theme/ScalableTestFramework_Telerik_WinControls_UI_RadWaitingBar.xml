<?xml version="1.0" encoding="utf-8"?>
<XmlTheme xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" ThemeName="ScalableTestFramework" ThemeVersion="2.0">
  <BuilderRegistrations>
    <XmlStyleBuilderRegistration>
      <BuilderData xsi:type="XmlStyleSheet" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
        <PropertySettingGroups>
          <XmlPropertySettingGroup BasedOn="ProgressBarBorder,ProgressBarHorizontalFill,Padding(0;0;0;0)">
            <Selectors>
              <XmlVisualStateSelector VisualState="RadWaitingBarElement" />
            </Selectors>
          </XmlPropertySettingGroup>
          <XmlPropertySettingGroup BasedOn="ProgressBarVertictalFill,ProgressBarBorder,Padding(0;0;0;0)">
            <Selectors>
              <XmlVisualStateSelector VisualState="RadWaitingBarElement.IsVertical" />
            </Selectors>
          </XmlPropertySettingGroup>
          <XmlPropertySettingGroup BasedOn="ProgressBarIndicator1HorizontalFill">
            <Selectors>
              <XmlVisualStateSelector VisualState="WaitingBarIndicatorElement" />
            </Selectors>
          </XmlPropertySettingGroup>
          <XmlPropertySettingGroup BasedOn="ProgressBarIndicator1VerticalFill">
            <Selectors>
              <XmlVisualStateSelector VisualState="WaitingBarIndicatorElement.IsVertical" />
            </Selectors>
          </XmlPropertySettingGroup>
          <XmlPropertySettingGroup BasedOn="DefaultForeColor,DefaultFont">
            <Selectors>
              <XmlTypeSelector ElementType="Telerik.WinControls.UI.WaitingBarTextElement" />
            </Selectors>
          </XmlPropertySettingGroup>
          <XmlPropertySettingGroup BasedOn="ProgressBarIndicator1VerticalFill">
            <PropertySettings>
              <XmlPropertySetting Property="Telerik.WinControls.UI.LightVisualElement.DrawFill" Value="False" />
              <XmlPropertySetting Property="Telerik.WinControls.UI.WaitingBarSeparatorElement.SweepAngle" Value="60" />
              <XmlPropertySetting Property="Telerik.WinControls.UI.WaitingBarSeparatorElement.SeparatorWidth" Value="20" />
              <XmlPropertySetting Property="Telerik.WinControls.UI.WaitingBarSeparatorElement.StepWidth" Value="20" />
            </PropertySettings>
            <Selectors>
              <XmlVisualStateSelector VisualState="WaitingBarSeparator.IsVertical" />
            </Selectors>
          </XmlPropertySettingGroup>
          <XmlPropertySettingGroup BasedOn="ProgressBarIndicator1HorizontalFill">
            <PropertySettings>
              <XmlPropertySetting Property="Telerik.WinControls.UI.WaitingBarSeparatorElement.SeparatorWidth" Value="20" />
              <XmlPropertySetting Property="Telerik.WinControls.UI.WaitingBarSeparatorElement.StepWidth" Value="20" />
              <XmlPropertySetting Property="Telerik.WinControls.UI.WaitingBarSeparatorElement.SweepAngle" Value="60" />
              <XmlPropertySetting Property="Telerik.WinControls.UI.LightVisualElement.DrawFill" Value="False" />
            </PropertySettings>
            <Selectors>
              <XmlVisualStateSelector VisualState="WaitingBarSeparator" />
            </Selectors>
          </XmlPropertySettingGroup>
        </PropertySettingGroups>
      </BuilderData>
      <StylesheetRelations>
        <RadStylesheetRelation ControlType="Telerik.WinControls.UI.RadWaitingBar" ElementType="Telerik.WinControls.RootRadElement" />
        <RadStylesheetRelation RegistrationType="ElementTypeDefault" ElementType="Telerik.WinControls.UI.RadWaitingBarElement" />
      </StylesheetRelations>
    </XmlStyleBuilderRegistration>
  </BuilderRegistrations>
</XmlTheme>
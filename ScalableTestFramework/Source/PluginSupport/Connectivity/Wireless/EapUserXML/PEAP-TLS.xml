			<baseEap:Type>25</baseEap:Type>
			<MsPeap:EapType>
				<MsPeap:RoutingIdentity>{0}</MsPeap:RoutingIdentity> <!-- NOTE: Might be incorrect. -->
				<baseEap:Eap>
					<baseEap:Type>13</baseEap:Type>
					<eapTls:EapType>
						<eapTls:Username>{1}\{0}</eapTls:Username>
						<eapTls:UserCert />
					</eapTls:EapType>
				</baseEap:Eap>
			</MsPeap:EapType>
		</baseEap:Eap>
	</Credentials>
</EapHostUserCredentials>


